#!/usr/bin/ksh

ENABLEDIR=/afs/eda.fishkill.ibm.com/data/edainfra/tools/enablement/prod

echo "linking start-commit to prod"
ln -s $ENABLEDIR/bin/start-commit start-commit

echo "linking pre-commit to prod"
ln -s $ENABLEDIR/bin/pre-commit pre-commit

echo "linking post-commit to prod"
ln -s $ENABLEDIR/bin/post-commit post-commit

echo "linking pre-revprop-change to prod"
ln -s $ENABLEDIR/bin/pre-revprop-change pre-revprop-change

